{
    "manifest_version": 3,
    "name": "HighSeas Doubloons",
    "version": "1.5.1",
    "description": "A simple extension that adds some new features to the HighSeas-Shipyard page",
    "content_scripts": [
        {
            "matches": [
                "https://highseas.hackclub.com/*",
                "https://high-seas.hackclub.dev/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
      "16": "./icon-x48.png",
      "48": "./icon-x48.png",
      "128": "./icon-x48.png"
    },
    "action": {
      "default_icon": {
        "16": "./icon-x48.png",
        "48": "./icon-x48.png",
        "128": "./icon-x48.png"
      },
      "default_title": "HighSeas Doubloons Plus Branch",
      "default_popup": "popup.html"
    },
    "permissions" : [
      "storage"
    ]
}